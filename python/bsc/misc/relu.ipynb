   "source": [
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "x = np.linspace(-5, 5, 100)\n",
    "y = np.maximum(0, x)\n",
    "\n",
    "plt.plot(x, y)\n",
    "plt.xlabel('x')\n",
    "plt.ylabel('f(x) = max(0,x)')\n",
    "plt.title('Rectified Linear Unit (ReLU)')\n",
    "plt.grid(True)\n",
    "plt.show()\n"
   ]
  }
